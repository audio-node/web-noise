{
  "name": "@web-noise/core",
  "version": "0.0.20",
  "source": "index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.js",
      "require": "./dist/main.js",
      "types": "./dist/types.d.ts"
    },
    "./constants": {
      "import": "./dist/esm/constants.js",
      "require": "./dist/cjs/constants.js",
      "types": "./dist/types.d.ts"
    },
    "./components": {
      "import": "./dist/esm/components.js",
      "require": "./dist/cjs/components.js",
      "types": "./dist/types.d.ts"
    },
    "./lib": {
      "import": "./dist/esm/lib.js",
      "require": "./dist/cjs/lib.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "targets": {
    "main": {
      "source": "index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": [
        "@xyflow/react/dist/style.css"
      ],
      "optimize": true,
      "outputFormat": "commonjs"
    },
    "module": {
      "source": "index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": [
        "@xyflow/react/dist/style.css"
      ],
      "optimize": true,
      "outputFormat": "esmodule"
    },
    "types": {
      "source": "index.ts",
      "distDir": "dist",
      "isLibrary": true
    },
    "constantsCjs": {
      "source": "constants.ts",
      "distDir": "dist/cjs",
      "isLibrary": true,
      "optimize": true,
      "includeNodeModules": false,
      "outputFormat": "commonjs"
    },
    "constantsEsm": {
      "source": "constants.ts",
      "distDir": "dist/esm",
      "isLibrary": true,
      "optimize": true,
      "includeNodeModules": false,
      "outputFormat": "esmodule"
    },
    "componentsCjs": {
      "source": "components.ts",
      "distDir": "dist/cjs",
      "includeNodeModules": false,
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "commonjs"
    },
    "componentsEsm": {
      "source": "components.ts",
      "distDir": "dist/esm",
      "includeNodeModules": false,
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "esmodule"
    },
    "libCjs": {
      "source": "lib.ts",
      "distDir": "dist/cjs",
      "isLibrary": true,
      "optimize": true,
      "includeNodeModules": false,
      "outputFormat": "commonjs"
    },
    "libEsm": {
      "source": "lib.ts",
      "distDir": "dist/esm",
      "isLibrary": true,
      "optimize": true,
      "includeNodeModules": false,
      "outputFormat": "esmodule"
    }
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@xyflow/react": "^12.9.2",
    "CanvasSpliner": "github:jonathanlurie/CanvasSpliner",
    "hotkeys-js": "^3.13.1",
    "js-file-download": "^0.4.12",
    "jsondiffpatch": "^0.5.0",
    "marked": "^4.1.1",
    "nanoid": "^3.3.7",
    "rc-slider": "^10.3.0",
    "re-resizable": "^6.9.9",
    "react-contexify": "^5.0.0",
    "react-draggable": "^4.4.5",
    "react-file-drop": "^3.1.6",
    "react-grid-layout": "^1.3.4",
    "react-icons": "^4.10.1",
    "react-modern-drawer": "^1.2.0",
    "use-debounce": "^8.0.4",
    "zustand": "^4.4.1"
  },
  "scripts": {
    "test": "jest",
    "build": "parcel build --no-scope-hoist --no-cache"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/react": "^19.1.12",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^19.1.9",
    "@types/react-grid-layout": "^1.3.2",
    "@types/react-modal": "^3.13.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^5.6.3",
    "web-audio-engine": "^0.13.4"
  },
  "peerDependencies": {
    "@web-noise/fetch": "*",
    "@web-noise/patch": "*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
