{
  "name": "@web-noise/core",
  "version": "0.0.6",
  "source": "index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.js",
      "require": "./dist/main.js",
      "types": "./dist/types.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.js",
      "require": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "targets": {
    "main": {
      "isLibrary": true,
      "includeNodeModules": [
        "@web-noise/fetch",
        "@web-noise/patch"
      ],
      "optimize": true,
      "outputFormat": "commonjs"
    },
    "module": {
      "isLibrary": true,
      "includeNodeModules": [
        "@web-noise/fetch",
        "@web-noise/patch"
      ],
      "optimize": true,
      "outputFormat": "esmodule"
    },
    "types": {
      "source": "index.ts",
      "isLibrary": true
    },
    "constants": {
      "source": "constants.ts",
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "commonjs"
    }
  },
  "dependencies": {
    "hotkeys-js": "^3.13.1",
    "js-file-download": "^0.4.12",
    "jsondiffpatch": "^0.5.0",
    "marked": "^4.1.1",
    "nanoid": "^3.3.7",
    "re-resizable": "^6.9.9",
    "react-contexify": "^5.0.0",
    "react-draggable": "^4.4.5",
    "react-file-drop": "^3.1.6",
    "react-grid-layout": "^1.3.4",
    "react-icons": "^4.10.1",
    "react-modern-drawer": "^1.2.0",
    "reactflow": "^11.10.3",
    "zustand": "^4.4.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0"
  },
  "scripts": {
    "test": "jest",
    "build": "parcel build --no-scope-hoist --no-cache"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/react-grid-layout": "^1.3.2",
    "@types/react-modal": "^3.13.1",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "web-audio-engine": "^0.13.4",
    "typescript": "^5.6.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9"
  },
  "peerDependencies": {
    "@web-noise/fetch": "*",
    "@web-noise/patch": "*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
