{
  "name": "@web-noise/base-nodes",
  "version": "0.0.7",
  "source": "index.ts",
  "type": "module",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "base": "dist/base/main.js",
  "webAudio": "dist/webAudio.js",
  "webAudioBase": "dist/base/webAudio.js",
  "patch": "dist/patch.js",
  "patchBase": "dist/base/patch.js",
  "webNoise": "dist/webNoise.js",
  "webNoiseBase": "dist/base/webNoise.js",
  "scriptNodes": "dist/scriptNodes.js",
  "scriptNodesBase": "dist/base/scriptNodes.js",
  "targets": {
    "base": {
      "source": "base.ts",
      "distDir": "dist/base",
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": true
    },
    "webAudio": {
      "source": "src/webAudio/index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": false,
      "optimize": false
    },
    "webAudioBase": {
      "source": "src/webAudio/base.ts",
      "distDir": "dist/base",
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": true
    },
    "patch": {
      "source": "src/patchNodes/index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": false
    },
    "patchBase": {
      "source": "src/patchNodes/base.ts",
      "distDir": "dist/base",
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": true
    },
    "webNoise": {
      "source": "src/webNoise/index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": [
        "phaze-vocoder"
      ],
      "optimize": false
    },
    "webNoiseBase": {
      "source": "src/webNoise/base.ts",
      "distDir": "dist/base",
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": true
    },
    "scriptNodes": {
      "source": "src/scriptNodes/index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": [
        "typescript"
      ],
      "optimize": false
    },
    "scriptNodesBase": {
      "source": "src/scriptNodes/base.ts",
      "distDir": "dist/base",
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": true
    },
    "module": {
      "source": "index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": [
        "phaze-vocoder",
        "typescript"
      ],
      "optimize": true
    },
    "main": {
      "source": "index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": [
        "phaze-vocoder",
        "typescript"
      ],
      "optimize": true
    },
    "types": {
      "source": "index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": false
    }
  },
  "exports": {
    ".": {
      "import": "./dist/module.js",
      "require": "./dist/main.js",
      "types": "./dist/types.d.ts"
    },
    "./webAudio": {
      "import": "./dist/webAudio.js",
      "types": "./dist/webAudio.d.ts"
    },
    "./base/webAudio": {
      "import": "./dist/base/webAudio.js",
      "types": "./dist/base/webAudio.d.ts"
    },
    "./webNoise": {
      "import": "./dist/webNoise.js",
      "types": "./dist/webNoise.d.ts"
    },
    "./patch": {
      "import": "./dist/patch.js",
      "types": "./dist/patch.d.ts"
    },
    "./scriptNodes": {
      "import": "./dist/scriptNodes.js",
      "types": "./dist/scriptNodes.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "generate:node": "generate-node",
    "build": "parcel build --no-scope-hoist --no-cache"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.3.3",
    "@tonaljs/note": "^4.10.0",
    "@tonaljs/range": "^4.8.1",
    "@tonaljs/scale": "^4.12.2",
    "@tonaljs/tonal": "^4.10.0",
    "CanvasSpliner": "github:jonathanlurie/CanvasSpliner",
    "aubiojs": "^0.2.1",
    "audiobuffer-to-wav": "^1.0.0",
    "fft-js": "^0.0.12",
    "js-file-download": "^0.4.12",
    "marked": "^4.1.1",
    "monaco-editor": "^0.38.0",
    "phaze-vocoder": "github:olvb/phaze",
    "prismjs": "^1.29.0",
    "rc-slider": "^10.3.0",
    "re-resizable": "^6.9.9",
    "react-envelope-graph": "^0.1.4",
    "react-grid-layout": "^1.3.4",
    "react-icons": "^4.10.1",
    "react-piano": "^3.1.3",
    "react-simple-code-editor": "^0.13.0",
    "splines": "jonathanlurie/splines",
    "use-animation-frame": "^0.1.4",
    "use-debounce": "^8.0.4"
  },
  "devDependencies": {
    "@types/marked": "^4.0.7",
    "@types/webmidi": "^2.1.0",
    "benchmark": "^2.1.4",
    "microtime": "^3.1.1",
    "ts-jest": "^29.1.0"
  },
  "peerDependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@web-noise/core": "*",
    "@web-noise/fetch": "*",
    "@web-noise/patch": "*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.6.3"
  }
}
