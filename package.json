{
  "name": "web-noise",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "targets": {
    "site": {
      "source": "src/index.html",
      "distDir": "./docs",
      "publicUrl": "/web-noise",
      "scopeHoist": false
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@web-noise/base-nodes": "*",
    "@web-noise/core": "*",
    "@web-noise/fetch": "*",
    "@web-noise/generator": "*",
    "@web-noise/patch": "*",
    "react": "^19.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.1.1",
    "react-qr-code": "^2.0.8",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build --target site",
    "clean": "rm -rf node_modules .parcel-cache yarn.lock packages/**/dist/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/config-default": "^2.10.1",
    "@parcel/optimizer-blob-url": "^2.10.0",
    "@parcel/optimizer-data-url": "^2.10.0",
    "@parcel/packager-raw-url": "^2.10.1",
    "@parcel/packager-ts": "^2.10.1",
    "@parcel/transformer-inline": "^2.10.0",
    "@parcel/transformer-inline-string": "^2.10.1",
    "@parcel/transformer-typescript-tsc": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.10.1",
    "@parcel/transformer-webmanifest": "^2.10.1",
    "@parcel/transformer-worklet": "^2.10.1",
    "@parcel/validator-typescript": "^2.12.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/audioworklet": "^0.0.23",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^19.1.12",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^19.1.9",
    "assert": "^2.0.0",
    "buffer": "^5.5.0||^6.0.0",
    "parcel": "^2.10.0",
    "path-browserify": "^1.0.0",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "ts-loader": "^9.3.1",
    "typescript": "^5.6.3",
    "worklet-loader": "^2.0.0"
  }
}
